function pixels = histeq( image, nacc, verbose )
if nargin == 1
  nacc = 256;
elseif nargin == 2
  verbose = -1;
end
binsize = 256/nacc;
temp = linspace(binsize/2,256-(binsize/2),nacc);

[nk,rk] = hist(image(:),temp);
prob = nk/sum(nk);

if verbose > 0
	figure('name','Histogram of Original');
    hist(image(:),temp)
    [nk,rk] = hist(image(:),temp)
end
cu = cumsum(prob);

ma = max(image(:));
mi = min(image(:));

temp = image/(ma-mi)*nacc;

temp = ceil(temp);

[n,m] = size(temp)
for i = 1:n
   for j = 1:
   end
end

pixels = cu(temp(:,:));


end

